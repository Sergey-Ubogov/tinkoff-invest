{
    "name": "tinkoff-invest",
    "homepage": "http://sergey-ubogov.github.io/tinkoff-invest",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "predeploy": "npm run build",
        "deploy": "gh-pages -d build",
        "eslint": "eslint .",
        "precommit": "lint-staged"
    },
    "dependencies": {
        "@tinkoff/invest-openapi-js-sdk": "1.5.0",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "react-scripts": "5.0.1"
    },
    "lint-staged": {
        "*.{js, jsx}": [
            "prettier --write",
            "npm run eslint -- --fix",
            "git add"
        ]
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "eslint": "8.57.0",
        "eslint-config-prettier": "9.1.0",
        "eslint-config-silence": "2.0.0",
        "eslint-plugin-prettier": "5.1.3",
        "gh-pages": "6.1.1",
        "husky": "9.0.11",
        "lint-staged": "15.2.2",
        "prettier": "3.2.5",
        "prettier-eslint": "16.3.0"
    }
}